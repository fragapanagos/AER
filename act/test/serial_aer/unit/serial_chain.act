import "serial_aer.act";

pint M=3;
pint N=4;
globals g;
bool Reset = g.sReset;

eMx1ofN<M, N> src;
// break up eMx1ofN into M e1ofN channels for parsing
e1of<N> src_[M];
(; m : M :
    src_[m].b = src.b[m];
    src_[m].e = src.e;
)
aer::interface::SERIAL_CHAIN<M, N> serial(g, src,,,);
aer::interface::OUT_e1ofN<N> out(g, serial.yo, serial.yi, serial.y,);
SINK_e1ofN<N> snk(g, out.y);
